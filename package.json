{
  "name": "jss-expand",
  "description": "JSS plugin that gives you a better syntax than CSS.",
  "version": "1.5.0",
  "author": {
    "name": "Pavel Davydov",
    "email": "typical000@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:cssinjs/jss-expand.git"
  },
  "keywords": [
    "cssinjs",
    "css-in-js",
    "css in js",
    "jss",
    "plugin",
    "expand"
  ],
  "scripts": {
    "all": "npm run lint && npm run test && npm run build",
    "build": "npm run clean && npm run build:lib && npm run build:max && npm run build:min",
    "clean": "rimraf ./lib/*",
    "build:lib": "babel src --out-dir lib",
    "build:max": "cross-env NODE_ENV=development webpack src/index.js dist/jss-expand.js",
    "build:min": "cross-env NODE_ENV=production webpack src/index.js dist/jss-expand.min.js",
    "lint": "eslint ./src",
    "lint:staged": "lint-staged",
    "bench": "cross-env BENCHMARK=true npm run test",
    "test": "karma start --single-run",
    "test:watch": "karma start",
    "prepublish": "npm run all"
  },
  "license": "MIT",
  "main": "./lib/index.js",
  "devDependencies": {
    "babel-cli": "^6.14.0",
    "babel-core": "^6.14.0",
    "babel-eslint": "^6.1.2",
    "babel-loader": "^6.2.5",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-stage-0": "^6.5.0",
    "cross-env": "^2.0.1",
    "eslint": "^2.13.1",
    "eslint-config-airbnb": "^8.0.0",
    "eslint-config-jss": "^1.1.0",
    "eslint-plugin-import": "^1.14.0",
    "eslint-plugin-jsx-a11y": "^1.5.5",
    "eslint-plugin-react": "^5.2.2",
    "expect.js": "^0.3.1",
    "json-loader": "^0.5.4",
    "jss": "^5.5.0",
    "karma": "^1.2.0",
    "karma-benchmark": "^0.6.0",
    "karma-benchmark-reporter": "^0.1.1",
    "karma-browserstack-launcher": "^1.1.1",
    "karma-chrome-launcher": "^2.0.0",
    "karma-coverage": "^1.1.1",
    "karma-firefox-launcher": "^1.0.0",
    "karma-mocha": "^1.1.1",
    "karma-mocha-reporter": "^2.1.0",
    "karma-safari-launcher": "^1.0.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^1.8.0",
    "lint-staged": "^3.0.1",
    "lodash.assign": "^4.2.0",
    "mocha": "^3.0.2",
    "pre-commit": "^1.1.3",
    "rimraf": "^2.5.4"
  },
  "peerDependencies": {
    "jss": "^5.5.0"
  },
  "lint-staged": {
    "./src/*.js": [
      "eslint --fix",
      "git add"
    ]
  },
  "pre-commit": "lint:staged"
}
